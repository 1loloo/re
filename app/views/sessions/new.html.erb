<div class="form_font">  
	 <%= form_for(:session, url: sessions_path) do |f| %>

      <%= f.label :email %>
      <%= f.text_field :email %>

      <%= f.label :password,"Пароль" %>
      <%= f.password_field :password %>

      <%= f.submit "Войти" %>
    <% end %>
		<br />
    <p>Новый пользователь? <%= link_to "Регистрация!", signup_path %></p>
</div>
